{
    "name": "gist",
    "version": "1.9.8",
    "description": "## Server & Front-end Code Repository",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/schemadesign/gist.git"
    },
    "private": true,
    "engines": {
        "node": ">=8.11.2",
        "npm": ">=6.1.0"
    },
    "scripts": {
        "start": "node app.js",
        "monitor": "nodemon -x 'node app.js || touch app.js'",
        "monitor_inspect": "nodemon --inspect=0.0.0.0:9229 app.js",
        "profiler": "node --prof app.js",
        "postinstall": "bower install --allow-root && grunt build",
        "seed": "seed",
        "test:backend": "cross-env NODE_ENV=testing jest --forceExit --runInBand --config=./internals/testing/backend/config.json",
        "test:backend:debugger": "cross-env NODE_ENV=testing node --inspect ./node_modules/.bin/jest --watch --forceExit --runInBand --config=./internals/testing/backend/config.json",
        "test:frontend": "cross-env NODE_ENV=testing jest --forceExit --runInBand --config=./internals/testing/frontend/config.json",
        "test:backend:watch": "npm run test:backend -- --watch",
        "test:frontend:watch": "npm run test:frontend -- --watch",
        "test": "npm run test:backend && npm run test:frontend",
        "lint": "eslint"
    },
    "nyc": {
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "cache": true,
        "exclude": [
            "tests/**",
            "user/**",
            "config/**"
        ]
    },
    "browserslist": [
        "> 0.5%",
        "IE 9",
        "IE 10",
        "IE 11"
    ],
